{
  "name": "Zizle-backend",
  "version": "1.0.0",
  "description": "",
  "author": "Arslan Majeed",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/src/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d src/config/orm.config-migrations.ts",
    "migration:generate": "npm run typeorm:cli migration:generate src/migrations",
    "migration:create": "npm run typeorm migration:create",
    "migration:run": "npm run typeorm:cli migration:run",
    "migration:revert": "npm run typeorm:cli migration:revert",
    "db:migrate": "npm run typeorm:cli migratin:run",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config -n src/config/orm.config.ts",
    "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed -n src/config/orm.config.ts",
    "tslint": "tslint -p tsconfig.json -c tslint.json 'src/**/*.ts'",
    "eslint": "eslint --fix src",
    "prettier": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/common": "^9.2.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-socket.io": "^9.2.1",
    "@nestjs/swagger": "^6.1.3",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjs/websockets": "^9.2.1",
    "@ngneat/falso": "^6.4.0",
    "@stripe/react-stripe-js": "^1.7.1",
    "@stripe/stripe-js": "^1.27.0",
    "@types/multer": "^1.4.7",
    "@types/otp-generator": "^4.0.0",
    "@types/passport-jwt": "^3.0.8",
    "@types/uuid": "^9.0.0",
    "axios": "^1.2.2",
    "bcryptjs": "^2.4.3",
    "buffer-to-stream": "^1.0.0",
    "class-sanitizer": "^1.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cloudinary": "^1.36.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "google-auth-library": "^8.7.0",
    "handlebars": "^4.7.7",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "node-schedule": "^2.1.1",
    "nodemailer": "^6.8.0",
    "otp-generator": "^4.0.0",
    "passport": "^0.6.0",
    "passport-google": "^0.3.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "randomstring": "^1.2.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "socket.io": "^4.5.4",
    "stream": "^0.0.2",
    "typeorm": "^0.3.11",
    "typeorm-seeding": "^1.6.1",
    "uuid": "^9.0.0",
    "webpack": "^5.88.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6.4.7",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.0.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "28.1.3",
    "prettier": "^2.8.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "source-map-support": "^0.5.20",
    "stripe": "^8.215.0",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "moduleNameMapper": {
    "@config/(.*)": "<rootDir>/src/config/$1",
    "@config": "<rootDir>/src/config",
    "@lib/utils/(.*)": "<rootDir>/libs/utils/src/$1",
    "@lib/utils": "<rootDir>/libs/utils/src",
    "@lib/types/(.*)": "<rootDir>/libs/types/src/$1",
    "@lib/types": "<rootDir>/libs/types/src",
    "@lib/dtos/(.*)": "<rootDir>/libs/dtos/src/$1",
    "@lib/dtos": "<rootDir>/libs/dtos/src",
    "@lib/constants/(.*)": "<rootDir>/libs/constants/src/$1",
    "@lib/constants": "<rootDir>/libs/constants/src"
  }
}
